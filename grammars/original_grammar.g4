grammar abap_vacek;


// operatory jazyka
DECLARATION_OP: DATA | FIELDSYMBOL;
CONSTRUCTOR_OP: NEW | VALUE | CONV | CORRESPONDING | CAST | REF | EXACT | REDUCE | FILTER | COND | SWITCH;
ASSIGNMENT_OP: EQ_OP | '?=';
ARITHMETIC_OP: PLUS_OP | MINUS_OP | MULTIPLY_OP | DIVISION_OP | DIV | MOD | '**';
BIT_OP: BITAND | BITOR | BITXOR | BITNOT;
STRING_OP: '&&';
RELATIONAL_OP: EQ_OP | '<>' | LT_OP | GT_OP | '<=' | '>=' | BETWEEN;
BOOLEAN_OP: AND | OR | XOR | NOT;
LITERAL_OP: '&';

// datove typy
DATA_TYPE: ( NUMERIC_TYPE | CHARACTER_TYPE | BYTE_TYPE | DATETIME_TYPE );
NUMERIC_TYPE : ( 'b' | 's' | 'i' | 'int8' | 'p' | 'decfloat16' | 'decfloat34' | 'f' );
CHARACTER_TYPE : ( 'c' (WS LENGTH WS INTEGER)? | 'n' (WS LENGTH WS INTEGER)? | 'string' );
BYTE_TYPE : ( 'x' | 'xstring' );
DATETIME_TYPE : ( ' d' | ' t' );

// hodnoty promennych
INTEGER: NUM | NUM1 NUM*;
FLOAT: INTEGER DOT NUM+;
STRING: '\'' ( 'a'..'z' | 'A'..'Z' | '0'..'9'  | '_'  | ' ' | '.'  | ','  | ':' | '-')* '\'';

EQ_OP: '=';
LT_OP: '<';
GT_OP: '>';
DOT: '.';
LPAREN: '(';
RPAREN: ')';
SQLPAREN: '[';
SQRPAREN: ']';
ARROW: '->';
PLUS_OP: '+';
MINUS_OP: '-';
MULTIPLY_OP: '*';
DIVISION_OP: '/';
CHAR_STR: 'a'..'z' | 'A'..'Z' | '_';
NUM: '0'..'9';
NUM1: '1'..'9';
NEWLINE_CHAR: ('\r\n'|'\n'|'\r') -> channel(HIDDEN);
WS : (' ' | '\t')+ -> skip;

PREFIX : '/' CHAR_STR CHAR_STR CHAR_STR+ '/';

