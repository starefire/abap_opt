prog : statement*;
statement : ( generic | declaration | assignment | loop_into | loop_assign | method_call | read_assign | select_into | select_into | st_if | st_case | comment | table_index );
generic : keyword ( operator | operand | addition )* DOT;
keyword : ABAP_WORD;
user_defined : (PREFIX)? ( CHAR_STR+ (CHAR_STR | INTEGER)* | CHAR_STR+ INTEGER* (data_type | ABAP_WORD) | (data_type | ABAP_WORD) (CHAR_STR | INTEGER)+ | CHAR_STR+ INTEGER* (data_type | ABAP_WORD) (CHAR_STR | INTEGER)+ | CHAR_STR* (data_type | ABAP_WORD) (CHAR_STR | INTEGER)* (data_type | ABAP_WORD)+ (CHAR_STR | INTEGER)* | CHAR_STR+ INTEGER* (data_type | ABAP_WORD) (CHAR_STR | INTEGER)* (data_type | ABAP_WORD)+ (CHAR_STR | INTEGER)* );
operand: user_defined;
operator: ( DECLARATION_OP | CONSTRUCTOR_OP | ASSIGNMENT_OP | ARITHMETIC_OP | BIT_OP | STRING_OP | RELATIONAL_OP | BOOLEAN_OP | LITERAL_OP );
data_type : DATA_TYPE;
addition: ABAP_WORD;
declaration: DECLARATION_OP LPAREN user_defined RPAREN ( EQ value )? DOT | DECLARATION_OP user_defined TYPE data_type DOT;
value: ( numeric | STRING | math_expression );
numeric: ( INTEGER | FLOAT );
math_expression
    : t ARITHMETIC_OP math_expression
    | t;
t
    : f ARITHMETIC_OP t
    | f;
f:
    | LPAREN math_expression RPAREN
    | numeric;
assignment : user_defined ASSIGNMENT_OP ( value | user_defined) DOT;
loop_into : LOOP AT user_defined INTO DECLARATION_OP LPAREN user_defined RPAREN DOT statement* ENDLOOP DOT;
loop_assign : LOOP AT user_defined ASSIGNING FIELDSYMBOL LPAREN user_defined RPAREN DOT statement* ENDLOOP DOT;
method_call : user_defined ARROW user_defined LPAREN ( (IMPORTING | EXPORTING | CHANGING) user_defined ASSIGNMENT_OP (user_defined | value | (DECLARATION_OP LPAREN user_defined RPAREN)) )* RPAREN DOT;

read_assign :
    READ TABLE user_defined
    ASSIGNING FIELDSYMBOL LPAREN user_defined RPAREN DOT;

select_into : SELECT select_param FROM user_defined INTO TABLE DATA LPAREN user_defined RPAREN where_param? DOT;
select_param : ( '*' | user_defined (',' user_defined)* );
where_param : WHERE where_op ( ( AND | OR) where_op )*;
where_op : user_defined RELATIONAL_OP value;
select_single : '?';
table_index : user_defined ASSIGNMENT_OP user_defined SQLPAREN (user_defined | INTEGER) ( KEY user_defined INDEX user_defined )? SQRPAREN DOT;
st_if : IF condition DOT statement* st_else_if* st_else? ENDIF DOT;
st_else_if : ELSEIF condition DOT statement*;
st_else : ELSE DOT statement*;
condition : user_defined RELATIONAL_OP ( user_defined | value );
st_case : CASE user_defined DOT st_case_when* ENDCASE DOT;
st_case_when : WHEN (value | OTHERS). statement*;
comment: (MULTIPLY_OP STRING MULTIPLY_OP);
